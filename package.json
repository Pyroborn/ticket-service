{
  "name": "ticket-service",
  "version": "1.0.0",
  "description": "Ticket management service",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start:local": "cp .env.local .env && npm start",
    "start:k8s": "cp .env.kubernetes .env && npm start",
    "dev": "nodemon index.js",
    "dev:local": "cp .env.local .env && npm run dev",
    "test": "jest --config jest.config.js --detectOpenHandles",
    "test:watch": "jest --config jest.config.js --watch",
    "test:coverage": "jest --config jest.config.js --coverage"
  },
  "keywords": [
    "ticket",
    "service",
    "microservice",
    "express",
    "mongodb"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "amqplib": "^0.10.3",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "mongoose": "^8.1.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.1.6",
    "nodemon": "^3.0.3",
    "supertest": "^6.3.4"
  }
}
